@model StorageDBCourseWork.Models.MovingItem

@{
    ViewBag.Title = "Просмотр";
}

<h2>Просмотр</h2>

<div>
    <h4>Перемещение</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayName("Откуда")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.StorageFrom.Name)
        </dd>

        <dt>
            @Html.DisplayName("Куда")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.StorageTo.Name)
        </dd>

        <dt>
            @Html.DisplayName("Дата перемещения")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Date)
        </dd>

    </dl>
</div>
<div>
    @Html.Partial("_StorageItems", Model)
</div>
<p>
    @Html.ActionLink("Редактировать", "Edit", new { id = Model.Id }) |
    @Html.ActionLink("Вернуться к списку", "Index")
</p>
